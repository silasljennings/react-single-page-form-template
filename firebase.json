{
  "emulators": {
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "firestore": {
      "port": 8000
    },
    "database": {
      "port": 9000
    },
    "auth": {
      "port": 9099
    },
    "hosting": {
      "port": 5005
    },
    "functions": {
      "port": 5001
    },
    "storage": {
      "port": 9199
    }
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
  "functions": [
    {
      "source": "../cashboard-cloud-functions/functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  }
}